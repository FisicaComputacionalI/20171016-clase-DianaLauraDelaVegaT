//Programa que suma los numeros impares entre el 1 y 10
//Autor: Diana Laura De la Vega Trinidad.
#include <iostream>
using namespace std;
int main()
{
    int sum=0;
    int count=1;
    return 0;
    while ( count < 10){
    if ((count %2)>0){
        sum=sum+count;
    }
    count ++;
    }
    cout<<sum<<endl;
    return 0;
}
